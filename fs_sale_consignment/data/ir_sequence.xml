<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">
        <record id="consignment_requisition_sequence" model="ir.sequence">
            <field name="name">Consignment Requisition Sequence</field>
            <field name="code">consignment.requisition</field>
            <field name="prefix">CNR/%(range_y)s/</field>
            <field name="padding">5</field>
            <field name="number_next" eval="1"/>
            <field name="number_increment" eval="1"/>
            <field name="use_date_range" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>