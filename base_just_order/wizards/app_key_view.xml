<odoo>
    <record id="app_key_view" model="ir.ui.view">
        <field name="name">app.key.view</field>
        <field name="model">app.key</field>

        <field name="arch" type="xml">
            <form string="App Key">
                <sheet>
                    <group>
                        <field name="key" string="Key"/>
                    </group>
                </sheet>
                <footer>
                    <button string="Save" name="store_in_jr" type="object" default_focus="1" class="btn-primary"/>
                    <button string="Cancel" class="btn-default" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <!-- <act_window id="action_barcode_generate"
        name="Generate Barcode"
        view_id="product_barcode_generate_view"
        res_model="product.barcode.generate"
        src_model="product.template"
        view_mode="form" target="new"/>
 -->
    <record id="action_app_key" model="ir.actions.act_window">
        <field name="name">App Key</field>
        <field name="res_model">app.key</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="base_just_order.app_key_view"/>
        <field name="target">new</field>
        <field name="binding_model_id" ref="base_just_order.model_jr_config"/>
        <field name="binding_view_types">form</field>
    </record>
</odoo>