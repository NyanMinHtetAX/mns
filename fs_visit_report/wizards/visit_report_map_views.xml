<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_visit_report_map_form" model="ir.ui.view">
        <field name="name">visit.report.map.form</field>
        <field name="model">visit.report.map</field>
        <field name="arch" type="xml">
            <form create="false" edit="false" delete="false">
                <sheet>
                    <field name="latitude" invisible="1"/>
                    <field name="longitude" invisible="1"/>
                    <field name="map_widget" widget="visit_route_map"/>
                </sheet>
            </form>
        </field>
    </record>
</odoo>