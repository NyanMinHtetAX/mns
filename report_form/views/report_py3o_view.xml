        <record id="a4_paper_format_customize_report" model="report.paperformat">
            <field name="name">A4 Customize Paper Format</field>
            <field name="default" eval="True"/>
            <field name="format">A4</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">20</field>
            <field name="margin_bottom">23</field>
            <field name="margin_left">5</field>
            <field name="margin_right">5</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">40</field>
            <field name="dpi">98</field>
            <field name="report_ids"
                   eval="[(6, 0, [ref('report_form.action_invoice_a4_pdf_report'),
                                  ref('report_form.action_26th_invoice_a4_pdf_report'),
                                  ref('report_form.action_cherry_invoice_a4_pdf_report'),
                                  ref('report_form.action_sale_marketing_invoice_a4_pdf_report'),
                                  ref('report_form.action_local_po_agreement_a4_pdf_report'),
                                  ref('report_form.action_production_order_summary_a4_pdf_report'),
                                  ref('report_form.action_foreign_po_agreement_a4_pdf_report'),
                                  ref('report_form.action_summary_a4_pdf_report')])]"/>
        </record>

