<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--Delivery Order ( Cargate & Driver ) By MPP 28 Nov 2022 -->
    <record id="action_delivery_order_b5_pdf_report" model="ir.actions.report">
        <field name="name">Delivery Order For Driver (B5)</field>
        <field name="model">stock.picking</field>
        <field name="report_type">py3o</field>
        <field name="report_name">report_form.delivery_order_b5_pdf_report</field>
        <field name="report_file">report_form.delivery_order_b5_pdf_report</field>
        <field name="py3o_filetype">pdf</field>
        <field name="module">report_form</field>
        <field name="py3o_template_fallback">reports/delivery_order_b5.odt</field>
    </record>
    <record id="action_delivery_order_b5_pdf_report_server" model="ir.actions.server">
        <field name="name">Delivery Order For Driver (B5)</field>
        <field name="model_id" ref="stock.model_stock_picking"/>
        <field name="state">code</field>
        <field name="code">
            action = records.get_delivery_order_b5_pdf_report()
        </field>
        <field name="binding_model_id" ref="stock.model_stock_picking"/>
        <field name="binding_type">report</field>
    </record>

