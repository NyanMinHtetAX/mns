<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="account_move_form_bill" model="ir.ui.view">
            <field name="name">account.move.form.bill</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="multi_uom.view_account_move_form_inherit"/>
            <field name="groups_id" eval="[(6, 0, [ref('user_access_right.group_account_price')])]"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook//page[1]//field[@name='invoice_line_ids']//tree//field[@name='multi_price_unit']" position="attributes">
                    <attribute name="attrs">{'readonly': [('product_type','!=','service')]}</attribute>
                </xpath>

            </field>
        </record>

    </data>
</odoo>