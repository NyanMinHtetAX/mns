<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="product_brand_id" model="ir.sequence">
            <field name="name">Product Brand Code ID</field>
            <field name="code">product.brand</field>
            <field name="prefix"></field>
            <field name="padding">3</field>
            <field name="number_next" eval="1"/>
            <field name="number_increment" eval="1"/>
            <field name="use_date_range" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>

        <record id="product_code_id" model="ir.sequence">
            <field name="name">Product Code ID</field>
            <field name="code">product.product</field>
            <field name="padding">4</field>
        </record>

    </data>
</odoo>
