<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">
        <record id="sale_stock_return_sequence" model="ir.sequence">
            <field name="name">Sale Stock Return Sequence</field>
            <field name="code">sale.stock.return</field>
            <field name="prefix">SSR/%(range_y)s/</field>
            <field name="padding">5</field>
            <field name="number_next" eval="1"/>
            <field name="number_increment" eval="1"/>
            <field name="use_date_range" eval="True"/>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>