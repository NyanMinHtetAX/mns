<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="account_move_inherit_report_template" model="ir.ui.view">
        <field name="name">account.move.view.form.inherit</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <field name="ref" position="after">
                <field name="report_template_id"/>
            </field>
        </field>
    </record>

    <record id="view_account_payment_form_inherit_report_template" model="ir.ui.view">
        <field name="name">account.payment.view.form.inherit</field>
        <field name="model">account.payment</field>
        <field name="inherit_id" ref="account.view_account_payment_form"/>
        <field name="arch" type="xml">
            <field name="payment_method_code" position="after">
                <field name="report_template_id"/>
            </field>
        </field>
    </record>

</odoo>
